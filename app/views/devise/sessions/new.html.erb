<section class="form-auth text-center">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <!-- <img class="mb-4" src="/docs/4.5/assets/brand/bootstrap-solid.svg" alt="" width="72" height="72"> -->
  <h1 class="h3 mb-3 font-weight-normal">Please Log in</h1>
    <%= f.email_field :email, autofocus: true, placeholder: "Email address",class:"form-control",id: "inputEmail" %>
    <%= f.password_field :password, placeholder: "Password", class:"form-control", id: "inputPassword" %>
    <div class="checkbox mb-3">
      <label>
        <%= f.check_box :remember_me %> Remember Me
      </label>
    </div>
      <%= f.submit "Sign in", class:"btn btn-lg btn-primary btn-block" %>
  <% end %>
  <%= render "devise/shared/links" %>
</section>
